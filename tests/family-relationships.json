{
  "@context": {
    "@vocab": "http://example.org/family#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "rel": "http://purl.org/vocab/relationship/"
  },
  "tests": {
    "0.1.1": {
      "name": "Spouse relationships",
      "description": "Test spouse relationships (explicit in data, reflexive)",
      "query": "SELECT ?spouse1 ?spouse2 WHERE { ?spouse1 :spouseOf ?spouse2 . } ORDER BY ?spouse1 ?spouse2",
      "expected": [
        { "spouse1": ":adam", "spouse2": ":eva" },
        { "spouse1": ":eva", "spouse2": ":adam" },
        { "spouse1": ":abel", "spouse2": ":ada" },
        { "spouse1": ":ada", "spouse2": ":abel" },
        { "spouse1": ":azura", "spouse2": ":antoine" },
        { "spouse1": ":antoine", "spouse2": ":azura" },
        { "spouse1": ":carol", "spouse2": ":carl" },
        { "spouse1": ":carl", "spouse2": ":carol" }
      ]
    },
    "0.1.2": {
      "name": "Direct parent-child relationships",
      "description": "Test direct parent-child relationships (explicit in data, inverse of child)",
      "query": "SELECT ?parent ?child WHERE { ?parent :parentOf ?child . } ORDER BY ?parent ?child",
      "expected": [
        { "parent": ":adam", "child": ":azura" },
        { "parent": ":adam", "child": ":abel" },
        { "parent": ":ada", "child": ":zela" },
        { "parent": ":abel", "child": ":zela" },
        { "parent": ":antoine", "child": ":fiona" },
        { "parent": ":antoine", "child": ":helen" },
        { "parent": ":azura", "child": ":bob" },
        { "parent": ":azura", "child": ":carol" },
        { "parent": ":azura", "child": ":helen" },
        { "parent": ":bob", "child": ":david" },
        { "parent": ":carol", "child": ":eliza" },
        { "parent": ":eva", "child": ":azura" },
        { "parent": ":eva", "child": ":abel" }
      ]
    },
    "0.1.3": {
      "name": "Child-parent relationships",
      "description": "Test child-parent relationships (inferred, inverse of parent)",
      "query": "SELECT ?child ?parent WHERE { ?child :childOf ?parent . } ORDER BY ?child ?parent",
      "expected": [
        { "child": ":abel", "parent": ":adam" },
        { "child": ":abel", "parent": ":eva" },
        { "child": ":azura", "parent": ":adam" },
        { "child": ":azura", "parent": ":eva" },
        { "child": ":bob", "parent": ":azura" },
        { "child": ":carol", "parent": ":azura" },
        { "child": ":david", "parent": ":bob" },
        { "child": ":eliza", "parent": ":carol" },
        { "child": ":fiona", "parent": ":antoine" },
        { "child": ":helen", "parent": ":antoine" },
        { "child": ":helen", "parent": ":azura" },
        { "child": ":zela", "parent": ":abel" },
        { "child": ":zela", "parent": ":ada" }
      ]
    },
    "0.1.4": {
      "name": "Adoptive parent relationships",
      "description": "Test adoptive parent relationships (subproperty of parentOf)",
      "query": "SELECT ?adoptiveParent ?adoptiveChild WHERE { ?adoptiveParent :adoptiveParentOf ?adoptiveChild . } ORDER BY ?adoptiveParent ?adoptiveChild",
